{% extends 'FreeNoteBundle:Backend:layout.html.twig' %}

{% block header %}
    <h1>Creating option<small> Use the form below to create a new option</small></h1>
{% endblock %}

{% block content %}
<form action="{{ path('free_note_backend_option_create') }}" method="post" class="form-horizontal" novalidate>
    {% include 'FreeNoteBundle:Backend/Option:_form.html.twig' %}
    <div class="form-actions">
       <input type="submit" value="create option" class="btn btn-primary btn-large" />
    </div>
</form>
{% endblock %}
