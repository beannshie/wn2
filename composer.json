{
    "repositories": {
        "sylius/assortment-bundle": {
            "type": "package",
            "package": {
                "name": "sylius/assortment-bundle",
                "type": "symfony-bundle",
                "description": "Manage products, their variants, properties and options in your Symfony2 application.",
                "keywords": ["shop", "ecommerce", "products", "product"],
                "homepage": "http://sylius.org",
                "license": "MIT",
                "authors": [
                    {
                        "name": "Paweł Jędrzejewski",
                        "email": "pjedrzejewski@diweb.pl",
                        "homepage": "http://pjedrzejewski.com"
                    },
                    {
                        "name": "Community contributions",
                        "homepage": "https://github.com/Sylius/SyliusAssortmentBundle/contributors"
                    }
                ],
                "minimum-stability": "dev",
                "require": {
                    "php": ">=5.3.3",

                    "symfony/framework-bundle": "~2.3",
                    "sylius/resource-bundle": "1.0.*@dev",
                    "stof/doctrine-extensions-bundle": "1.1.*"

                },
                "require-dev": {
                    "doctrine/orm": "~2.3",
                    "phpspec/phpspec2": "dev-develop",
                    "symfony/form": "~2.2",
                    "symfony/validator": "~2.2"
                },
                "config": {
                    "bin-dir": "bin"
                },
                "autoload": {
                    "psr-0": { "Sylius\\Bundle\\AssortmentBundle": "" }
                },
                "target-dir": "Sylius/Bundle/AssortmentBundle",
                "extra": {
                    "branch-alias": {
                        "dev-master": "0.1.x-dev"
                    }
                },

                "version": "dev-master",
                "source": {
                    "url": "https://github.com/Sylius/SyliusAssortmentBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "sylius/blogger-bundle": {
            "type": "package",
            "package": {
                "name": "sylius/blogger-bundle",
                "type": "symfony-bundle",
                "description": "Simple blogging bundle.",
                "keywords": ["blogger", "blog"],
                "homepage": "http://sylius.org",
                "license": "MIT",
                "authors": [
                    {
                        "name": "Paweł Jędrzejewski",
                        "email": "pjedrzejewski@diweb.pl",
                        "homepage": "http://pjedrzejewski.com"
                    },
                    {
                        "name": "Community contributions",
                        "homepage": "https://github.com/Sylius/SyliusBloggerBundle/contributors"
                    }
                ],
                "minimum-stability": "dev",
                "require": {
                    "php": ">=5.3.2",

                    "symfony/framework-bundle": "~2.3",
                    "sylius/resource-bundle": "1.0.*@dev",
                    "stof/doctrine-extensions-bundle": "1.1.*"
                },
                "require-dev": {
                    "phpspec/phpspec2": "dev-develop",
                    "symfony/form": "~2.1"
                },
                "config": {
                    "bin-dir": "bin"
                },
                "autoload": {
                    "psr-0": { "Sylius\\Bundle\\BloggerBundle": "" }
                },
                "target-dir": "Sylius/Bundle/BloggerBundle",
                "extra": {
                    "branch-alias": {
                        "dev-master": "0.1.x-dev"
                    }
                },

                "version": "dev-master",
                "source": {
                    "url": "https://github.com/Sylius/SyliusBloggerBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "sylius/categorizer-bundle": {
            "type": "package",
            "package": {
                "name": "sylius/categorizer-bundle",
                "type": "symfony-bundle",
                "description": "Easy categorizing in Symfony2 applications. Categorize any model you want, multiple catalogs supported.",
                "keywords": ["category", "categories", "catalog"],
                "homepage": "http://sylius.org",
                "license": "MIT",
                "authors": [
                    {
                        "name": "Paweł Jędrzejewski",
                        "email": "pjedrzejewski@diweb.pl",
                        "homepage": "http://pjedrzejewski.com"
                    },
                    {
                        "name": "Community contributions",
                        "homepage": "https://github.com/Sylius/SyliusCategorizerBundle/contributors"
                    }
                ],
                "require": {
                    "php": ">=5.3.2",

                    "symfony/symfony": "~2.3",
                    "stof/doctrine-extensions-bundle": "*",
                    "white-october/pagerfanta-bundle": "*"
                },
                "require-dev": {
                    "doctrine/orm": "*"
                },
                "autoload": {
                    "psr-0": { "Sylius\\Bundle\\CategorizerBundle": "" }
                },
                "target-dir": "Sylius/Bundle/CategorizerBundle",

                "version": "dev-master",
                "source": {
                    "url": "https://github.com/Sylius/SyliusCategorizerBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    },

    "autoload": {
        "psr-0": {
            "FreeNote\\FreeNoteBundle": "src/",
            "Sylius\\Behat":   "features/" }
    },
    "require": {
        "php":                                 ">=5.3.3",

        "doctrine/orm":                        ">=2.2.3,<2.4-dev",
        "fzaninotto/faker":                    "1.1.*",
        "symfony/symfony":                     "~2.3",
        "twig/extensions":                     "1.0.*",

        "avalanche123/imagine-bundle":         "dev-master",
        "doctrine/doctrine-bundle":            "1.2.*",
        "doctrine/doctrine-fixtures-bundle":   "2.2.*",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "stof/doctrine-extensions-bundle":     "1.1.*",

        "symfony/assetic-bundle":              "2.3.*",
        "symfony/swiftmailer-bundle":          "2.3.*",
        "symfony/monolog-bundle":              "2.3.*",
        "sensio/distribution-bundle":          "2.3.*",
        "sensio/generator-bundle":             "2.3.*",

        "friendsofsymfony/rest-bundle":        "0.13.*",
        "friendsofsymfony/user-bundle":        "2.0.*@dev",
        "friendsofsymfony/comment-bundle":     "2.0.*",
        "jms/serializer-bundle":               "0.12.*",
        "knplabs/knp-markdown-bundle":         "1.2.*",
        "knplabs/knp-menu-bundle":             "2.0.*",
        "mopa/bootstrap-bundle":               "3.x-dev",
        "liip/doctrine-cache-bundle":          "dev-master",

        "sylius/addressing-bundle":            "1.0.*@dev",
        "sylius/cart-bundle":                  "1.0.*@dev",
        "sylius/taxonomies-bundle":            "1.0.*@dev",
        "sylius/taxation-bundle":              "1.0.*@dev",
        "sylius/shipping-bundle":              "1.0.*@dev",
        "sylius/flow-bundle":                  "1.0.*@dev",
        "sylius/inventory-bundle":             "1.0.*@dev",
        "sylius/sales-bundle":                 "1.0.*@dev",
        "sylius/settings-bundle":              "1.0.*@dev",
        "sylius/assortment-bundle":            "dev-master",
        "sylius/blogger-bundle":               "dev-master",
        "sylius/categorizer-bundle":           "dev-master",

        "stfalcon/tinymce-bundle":             "dev-master"
    },
    "require-dev": {
        "behat/behat":                         "2.4.*",
        "behat/symfony2-extension":            "*",
        "behat/mink-extension":                "dev-master",
        "behat/mink-goutte-driver":            "dev-master",
        "phpspec/phpspec":                     "2.0.*@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink"
    }
}
