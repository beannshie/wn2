{
    "autoload": {
        "psr-0": {
            "FreeNote\\FreeNoteBundle": "src/",
            "Sylius\\Behat":   "features/" }
    },
    "require": {
        "php":                                 ">=5.3.3",

        "doctrine/orm":                        ">=2.2.3,<2.4-dev",
        "fzaninotto/faker":                    "1.1.*",
        "symfony/symfony":                     ">=2.2,<2.3-dev",
        "twig/extensions":                     "1.0.*",

        "avalanche123/imagine-bundle":         "dev-master",
        "doctrine/doctrine-bundle":            "1.2.*",
        "doctrine/doctrine-fixtures-bundle":   "2.1.*",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "stof/doctrine-extensions-bundle":     "1.1.*",

        "symfony/assetic-bundle":              "2.1.*",
        "symfony/swiftmailer-bundle":          "2.2.*",
        "symfony/monolog-bundle":              "2.2.*",
        "sensio/distribution-bundle":          "2.2.*",
        "sensio/generator-bundle":             "2.2.*",

        "friendsofsymfony/rest-bundle":        "0.11.*",
        "friendsofsymfony/user-bundle":        "1.3.1",
        "friendsofsymfony/comment-bundle":     "2.0.*",
        "jms/serializer-bundle":               "0.11.*",
        "knplabs/knp-markdown-bundle":         "1.2.*",
        "knplabs/knp-menu-bundle":             "2.0.*",
        "mopa/bootstrap-bundle":               "2.1.*",
        "liip/doctrine-cache-bundle":          "dev-master",

        "sylius/addressing-bundle":            "0.1.*",
        "sylius/assortment-bundle":            "0.1.*",
        "sylius/cart-bundle":                  "0.3.*",
        "sylius/categorizer-bundle":           "dev-master",
        "sylius/taxonomies-bundle":            "0.1.*",
        "sylius/taxation-bundle":              "0.1.*",
        "sylius/shipping-bundle":              "0.1.*",
        "sylius/flow-bundle":                  "0.1.*",
        "sylius/inventory-bundle":             "0.1.*",
        "sylius/sales-bundle":                 "0.1.*",
        "sylius/blogger-bundle":               "dev-master",
        "sylius/settings-bundle":              "0.1.*"
    },
    "require-dev": {
        "behat/mink-extension":                "dev-master",
        "behat/mink-goutte-driver":            "dev-master",
        "behat/symfony2-extension":            "dev-master",
        "phpspec/phpspec2":                    "1.0.*@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink"
    }
}
